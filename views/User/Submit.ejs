              <a class="text-secondary mr-4" href="/register">Reg</a>
             <a class="text-secondary mt-4" href="/login">Login</a>
            <%}%>
           
          </span>
        </div>
      </nav>
    </div>

    <div>
      <nav
        class="navbar navbar-expand-lg navbar-light bg-light border-top"
        style="background-color: #98b3ff"
      >
        <div
          class="collapse navbar-collapse justify-content-between"
          id="navbarNav"
        >
          <ul class="navbar-nav">
            <li class="nav-item active">
              <a class="nav-link" href="/Problem">Problem List</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/problemStatment/<%= pid %>"
                >Problem Statement</a
              >
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/submission/<%= pid %>"
                >Submission</a
              >
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/solution/<%= pid %>"
                >Solution</a
              >
            </li>
          </ul>
        </div>
      </nav>
    </div>


  <!-- Problem Last Submited Code  -->
  <div class="w-75 mx-auto">

    <% if(solve == "NO"){%>
      <div class="w-50 mx-auto text-secondary mt-5 text-center">
          <h4>NOT Solve Yet</h4>
      </div>
    <%}else{%>
      
      <div class="text-center text-success mt-3 mb-3">
        <h5>Latest Code Submission Info </h5>

        <div class="w-75 p-3 mx-auto text-dark">
            <table id="example" class="table table-striped table-hover mt-5" style="width:100;" >
              <thead>
                <tr>
                  <th scope="col">ID</th>
                  <th scope="col">Problem Name</th>
                  <th scope="col">Difficulty</th>
                  <th scope="col">Status</th>
                </tr>
              </thead>
              <tbody>
                 
               
              
                <tr>
                 <td><b><%= problem.spid %></b></td>
                
                 <td><a href="<%= Link %><%=problem.pid %>"> <%= problem.name %> </a></td>
                
                 <td><%= problem.difficulties %></td>
                
                 <td>
                  <% if(problem.status === 'Accepted'){ %>
                     <P class="text-success"><b><%=problem.status %></b></P>
                   <%}else if(problem.status === 'Wrong'){%>
                
                     <P class="text-danger"><b><%=problem.status %></b></P>
                   <%} %>
                </td>
             
                </tr> 
             
               
               
              </tbody>
            </table>
          </div>


    </div>
    <!-- Table Information -->
    <div></div>

    <div class="text-center text-success mt-3 mb-3">
        <h5>Submited Code</h5>
    </div>

    <div class="p-2 mx-auto">
     
 
        <div
          id="editor"
          title="css edit:"
          style="height: 600px; width: 100%; border-radius: 5px; border: 2px solid #b1b1b1;"
        >
       
  
    </div>
      




  </div>
    <script
      src="https://code.jquery.com/jquery-3.2.1.slim.min.js"
      integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js"
      integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js"
      integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
      crossorigin="anonymous"
    ></script>

    <script src="/static/Js/jquery.min.js"></script>
    <script src="/static/Js/popper.js"></script>
    <script src="/static/Js/bootstrap.min.js"></script>
    <script src="/static/Js/owl.carousel.min.js"></script>
    <script src="/static/Js/main.js"></script>
    <script
      defer
      src="https://static.cloudflareinsights.com/beacon.min.js/vaafb692b2aea4879b33c060e79fe94621666317369993"
      integrity="sha512-0ahDYl866UMhKuYcW078ScMalXqtFJggm7TmlUtp0UlD4eQk0Ixfnm5ykXKvGJNFjLMoortdseTfsRT8oCfgGA=="
      data-cf-beacon='{"rayId":"76881d67af7691a8","token":"cd0b4b3a733644fc843ef0b185f98241","version":"2022.11.0","si":100}'
      crossorigin="anonymous"
    ></script>

    <!-- Editor  Start -->

    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/ace/1.3.3/ace.js"
      type="text/javascript"
      charset="utf-8"
    ></script>

    <script>
      var editor = ace.edit("editor");
      editor.setTheme("ace/theme/Focus");
      editor.session.setMode("ace/mode/c_cpp");
      editor.setReadOnly(true);
     
    
      document.getElementById("editor").style.fontSize = "14px";

      

      // Content set After redirecting

      const painText = (a) => {
        if (a.charAt(0) === '"' && a.charAt(a.length - 1) === '"') {
          return a.substr(1, a.length - 2);
        }
        return "";
      };

      let code = `<%- JSON.stringify(problem.solution) %>`;
  

      code = painText(code);


      console.log("print =>" + code);

      const textArea = () => {

        editor.setValue(code, 1); // set edito value
      };
      textArea();
    </script>

  <%}%>
  </body>
</html>
