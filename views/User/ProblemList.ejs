</head>
<body>

    <div class="mx-auto">
        <nav
          class="navbar navbar-expand-lg navbar-light bg-light pl-2 pr-2"
          style="background-color: #e3f2fd"
        >
          <a class="navbar-brand text-primary" href="#">IUBAT_CPC</a>
          <button
            class="navbar-toggler"
            type="button"
            data-toggle="collapse"
            data-target="#navbarNav"
            aria-controls="navbarNav"
            aria-expanded="false"
            aria-label="Toggle navigation"
          >
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse justify-content-between" id="navbarNav">
            <ul class="navbar-nav">
              <li class="nav-item active">
                <a class="nav-link" href="/"
                  >Home <span class="sr-only">(current)</span></a
                >
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/problem"
                  >Problems</a
                >
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/achivements"
                  >Achivements</a
                >
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/notice"
                  >Notice</a
                >
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/event">Event</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/member"
                  >Members</a
                >
              </li>
              <li class="nav-item">
                <a class="nav-link" href="/seniors"
                  >Seniors</a
                >
              </li>
              
            </ul>
            <span>
              <a class="text-secondary" href="#">Reg</a>
              <a class="text-secondary" href="#">Login</a>
            </span>
             
          </div>
        </nav>
      </div>

      <div class="w-75 p-3 mx-auto">
        <table id="example" class="table table-striped table-hover mt-5" style="width:100;" >
          <thead>
            <tr>
              <th scope="col">ID</th>
              <th scope="col">Problem Name</th>
              <th scope="col">Difficulty</th>
              <th scope="col">Status</th>
            </tr>
          </thead>
          <tbody>
             
            <!-- <tr>
                  <th scope="row">1</th>
                  <td> <a href="/problemStatment/1"> Two Sum</a></td>
                  <td>Easy</td>
                  <td>No</td>
            </tr>
           -->
           <% data.forEach(function(item){ %>
            <tr>
             <td><b><%= item.pid %></b></td>
            
             <td><a href="<%= Link %><%=item.pid %>"> <%= item.name %> </a></td>
            
            
             <td>
              <% if(item.difficulties === 'Easy'){ %>
                 <P class="text-success"><%=item.difficulties %></P>
               <%}else if(item.difficulties === 'Worng'){%>
            
                 <P class="text-warning"><%= item.difficulties %></P>
               <%} else {%>
              
                 <P class="text-danger"><%= item.difficulties %></P>
               <%}%>
            </td>
            
             <td>
               <% if(item.status === 'Accepted'){ %>
                  <P class="text-success"><%=item.status %></P>
                <%}else if(item.status === 'Worng'){%>
             
                  <P class="text-danger"><%= item.status %></P>
                <%} else{%>
               
                  <P class="text-secondary"><%=notYet %></P>
                <%}%>
             </td>
         
            </tr> <% }) %>
         
           
           
          </tbody>
        </table>
      </div>
        



         





