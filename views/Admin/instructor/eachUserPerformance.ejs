        <!-- chart Script Start -->
        <script>

            let Ac = `<%- JSON.stringify(data.cntac) %>`;
            let Wa = `<%- JSON.stringify(data.cntwa) %>`;

           

            var chart = bb.generate({
              data: {
                columns: [
                  ["Acepted", +Ac],
                  ["Wrong", +Wa],
                  ["Time Limit", 2],
                ],
                type: "donut",
                onclick: function (d, i) {
                  console.log("onclick", d, i);
                },
                onover: function (d, i) {
                  console.log("onover", d, i);
                },
                onout: function (d, i) {
                  console.log("onout", d, i);
                },
              },
              donut: {
                title: "point <%=data.cntpoint%>",
              },
              bindto: "#donut-chart",
            });
          </script>

